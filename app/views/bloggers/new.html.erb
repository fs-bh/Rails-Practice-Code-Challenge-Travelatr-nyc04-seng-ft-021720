<h1>Create a New Blogger</h1>
<%= form_for @blogger do |f| %>
	<p><%= f.label :name %>: <%= f.text_field :name %></p>
	<p><%= f.label :age %>: <%= f.number_field :age %></p>
	<p><%= f.label :bio %>: <%= f.text_area :bio %></p>
	<p><%= f.submit %></p>
<% end %>
<p><%= link_to 'Cancel Creation', bloggers_path %></p>